<experiments>
  <experiment name="testBehaviorSpace1" repetitions="2">
    <stepLimit>2</stepLimit>
    <setup>set counter 0</setup>
    <go>repeat param1 [ repeat param2 [ set counter counter + 1 ] ]</go>
    <exitCondition>counter &gt; param2</exitCondition>
    <metrics>
      <metric>counter</metric>
    </metrics>
    <valueSets>
      <enumeratedValueSet variable="param1">
        <number>1</number>
        <number>2</number>
        <number>3</number>
      </enumeratedValueSet>
      <steppedValueSet variable="param2" first="1" step="1" last="5"/>
    </valueSets>
  </experiment>
  <experiment name="testBehaviorSpace2" repetitions="2" runMetricsEveryStep="false">
    <stepLimit>2</stepLimit>
    <setup>set counter 0</setup>
    <go>repeat param1 [ repeat param2 [ set counter counter + 1 ] ]</go>
    <exitCondition>counter &gt; param2</exitCondition>
    <metrics>
      <metric>counter</metric>
    </metrics>
    <valueSets>
      <enumeratedValueSet variable="param1">
        <number>1</number>
        <number>2</number>
        <number>3</number>
      </enumeratedValueSet>
      <steppedValueSet variable="param2" first="1" step="1" last="5"/>
    </valueSets>
  </experiment>
  <experiment name="testMultipleMetrics" >
    <stepLimit>4</stepLimit>
    <setup>set counter 0</setup>
    <go>set counter counter + 1</go>
    <exitCondition>counter &gt;= param1</exitCondition>
    <metrics>
      <metric>counter</metric>
      <metric>counter / 2</metric>
      <metric>counter / 10</metric>
    </metrics>
    <valueSets>
      <enumeratedValueSet variable="param1">
        <number>1</number>
        <number>2</number>
        <number>3</number>
      </enumeratedValueSet>
    </valueSets>
  </experiment>
